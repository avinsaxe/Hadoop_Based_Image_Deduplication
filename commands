duplicate_search -db default
duplicate_search -add /home/avinsaxe/Pictures
duplicate_search -show
duplicate_search -delete key value

duplicate_search -delete _id id1


duplicate_search -remove /home/avinsaxe/Pictures

duplicate_search -db reset